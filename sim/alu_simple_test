:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x63f5372a7da0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x63f5372a7f30 .scope module, "alu_simple_test" "alu_simple_test" 3 4;
 .timescale -9 -12;
v0x63f53727e7b0_0 .var "alu_op", 3 0;
v0x63f53727eba0_0 .var "carry_in", 0 0;
v0x63f5372ce300_0 .var "clk", 0 0;
v0x63f5372ce3a0_0 .var "operand_a", 31 0;
v0x63f5372ce480_0 .var "operand_b", 31 0;
v0x63f5372ce560_0 .var "result", 31 0;
v0x63f5372ce640_0 .var "rst_n", 0 0;
v0x63f5372ce700_0 .var "set_flags", 0 0;
E_0x63f53727d500 .event edge, v0x63f53727e7b0_0, v0x63f5372ce3a0_0, v0x63f5372ce480_0;
    .scope S_0x63f5372a7f30;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63f5372ce300_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63f5372ce640_0, 0, 1;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x63f53727e7b0_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63f5372ce700_0, 0, 1;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x63f5372ce3a0_0, 0, 32;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x63f5372ce480_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63f53727eba0_0, 0, 1;
    %end;
    .thread T_0, $init;
    .scope S_0x63f5372a7f30;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x63f5372ce300_0;
    %inv;
    %store/vec4 v0x63f5372ce300_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x63f5372a7f30;
T_2 ;
Ewait_0 .event/or E_0x63f53727d500, E_0x0;
    %wait Ewait_0;
    %load/vec4 v0x63f53727e7b0_0;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
    %cmp/u;
    %jmp/1 T_2.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_2.1, 6;
    %dup/vec4;
    %pushi/vec4 13, 0, 4;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x63f5372ce560_0, 0, 32;
    %jmp T_2.4;
T_2.0 ;
    %load/vec4 v0x63f5372ce3a0_0;
    %load/vec4 v0x63f5372ce480_0;
    %add;
    %store/vec4 v0x63f5372ce560_0, 0, 32;
    %jmp T_2.4;
T_2.1 ;
    %load/vec4 v0x63f5372ce3a0_0;
    %load/vec4 v0x63f5372ce480_0;
    %sub;
    %store/vec4 v0x63f5372ce560_0, 0, 32;
    %jmp T_2.4;
T_2.2 ;
    %load/vec4 v0x63f5372ce480_0;
    %store/vec4 v0x63f5372ce560_0, 0, 32;
    %jmp T_2.4;
T_2.4 ;
    %pop/vec4 1;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x63f5372a7f30;
T_3 ;
    %vpi_call/w 3 39 "$dumpfile", "alu_simple_test.vcd" {0 0 0};
    %vpi_call/w 3 40 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x63f5372a7f30 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63f5372ce640_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x63f5372ce640_0, 0, 1;
    %delay 20000, 0;
    %vpi_call/w 3 47 "$display", "=== Simple ALU Test ===" {0 0 0};
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x63f53727e7b0_0, 0, 4;
    %pushi/vec4 5, 0, 32;
    %store/vec4 v0x63f5372ce3a0_0, 0, 32;
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x63f5372ce480_0, 0, 32;
    %delay 10000, 0;
    %vpi_call/w 3 54 "$display", "ADD: %d + %d = %d (expected: 8)", v0x63f5372ce3a0_0, v0x63f5372ce480_0, v0x63f5372ce560_0 {0 0 0};
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x63f53727e7b0_0, 0, 4;
    %delay 10000, 0;
    %vpi_call/w 3 59 "$display", "SUB: %d - %d = %d (expected: 2)", v0x63f5372ce3a0_0, v0x63f5372ce480_0, v0x63f5372ce560_0 {0 0 0};
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x63f53727e7b0_0, 0, 4;
    %delay 10000, 0;
    %vpi_call/w 3 64 "$display", "MOV: %d (expected: 3)", v0x63f5372ce560_0 {0 0 0};
    %vpi_call/w 3 66 "$display", "\342\234\205 ALU Logic Test Complete" {0 0 0};
    %vpi_call/w 3 67 "$finish" {0 0 0};
